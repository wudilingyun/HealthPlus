<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.clienttest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

    <application
        android:name="com.example.clienttest.MainApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.clienttest.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.clienttest.register.RegisterActivity"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name="com.example.clienttest.authorization.SignInActivity"
            android:label="@string/title_activity_sign_in" >
        </activity>
        <activity
            android:name="com.example.clienttest.getprotectresource.GetProtectActivity"
            android:label="@string/title_activity_get_protect" >
        </activity>
        <activity
            android:name="com.example.clienttest.getpublicresource.GetPublicActivity"
            android:label="@string/title_activity_get_public" >
        </activity>
        <activity
            android:name="com.example.clienttest.MainTabWiget"
            android:label="@string/title_activity_main_tab_wiget" >
        </activity>
        <activity
            android:name="com.example.clienttest.profile.ProfileActivity"
            android:label="@string/title_activity_profile" >
        </activity>
    </application>

</manifest>
