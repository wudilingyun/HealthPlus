package com.vee.healthplus.widget.chart;


/**
 * touch事件发生后，支持对外发送事件消息的此类对象接口
 */
public interface ITouchEventNotify {
    public void notifyEventAll(GridChart chart); //通知全部ITouchEventResponse响应对象
    public void addNotify(ITouchEventResponse notify);
    public void removeNotify(int i); //index
    public void removeAllNotify();
}
